{
    "name": "elminson/db-logger",
    "description": "A powerful PHP package for logging SQL queries from Laravel applications, supporting both Eloquent and Query Builder instances, with flexible output options.",
    "type": "library",
    "license": "MIT",
    "version": "1.1.7",
    "authors": [
        {
            "name": "Elminson Deoleo",
            "email": "edeoleo@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/database": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "mockery/mockery": "^1.6",
        "pestphp/pest": "^3.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Elminson\\DQL\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Elminson\\DQL\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable"
}
